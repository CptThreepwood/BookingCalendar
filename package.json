    "@material-ui/lab": "^4.0.0-alpha.57",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "csstype": "^3.0.5",
    "firebase": "^8.0.2",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-dates": "^21.8.0",
